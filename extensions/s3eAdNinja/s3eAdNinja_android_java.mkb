#!/usr/bin/env mkb
# Builder mkb file for the java portion of the s3eAdNinja extension on android
platform JAVA

files
{
    (source/android)
    s3eAdNinja.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"


librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/adfonic-android-sdk.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/android-ads-adapters-jumptap.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/GoogleAdMobAdsSdk-6.0.1.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/huntmadsadaptor.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/JTAdTag.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/InMobiAdapter.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/libAdapterInMobi.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/MillennialAdapter.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/MMAdView.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/mobfox_combined_sdk.jar"
librarypath "$MARMALADE_ROOT/extensions/s3eAdNinja/mobfox_mediation_adapter.jar"


option output-name=lib/android/s3eAdNinja.jar
